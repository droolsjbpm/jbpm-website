- # Properties file read by jboss.org for the project listing
- # See http://www.jboss.org/help/awestructguide/projectpropertyfilestruction.html
- values = {}

- values['nodePath']= "/jbpm"
- values['projectName']= "jBPM"
- values['homePage']= site.canonicalBaseUrl
- values['description'] = site.description
- values['license']= "Apache-2.0"
- values['downloadsLink']= "#{site.canonicalBaseUrl}/download/download.html"
- values['docsLink']= "#{site.canonicalBaseUrl}/learn/documentation.html"

- # Community
- values['userForumLink']= "#{site.canonicalBaseUrl}/community/forum.html"
- values['chatLink']= "#{site.canonicalBaseUrl}/community/chat.html"
- values['blogLink']= "http://planet.jboss.org/#projects=jbpm"
- values['twitterLink']= "https://twitter.com/jbossjbpm"

- # Code
- values['githubLink']= "https://github.com/droolsjbpm"
- values['jiraLink']= "https://issues.jboss.org/browse/JBPM"
- values['hudsonLink']= "https://hudson.jboss.org/hudson/view/Drools%20jBPM/"

- for key in values.keys
    #{key}=#{values[key]}
